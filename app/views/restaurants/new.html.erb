<h1>New Restaurant Form</h1>

<%= form_for(@restaurant, html: {multipart: true}) do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :slug %>
    <%= f.text_field :slug %>
  </p>
  <%= f.submit "submit" %>
  <%end%>
