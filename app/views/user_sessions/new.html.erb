<h1>Login</h1>

<%= form_tag user_sessions_path, method: :post do %>
  <div>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </div>
  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div>
    <%= submit_tag "Login" %>
  </div
<% end %>

<%= link_to "<< Back to Items", items_path %>
